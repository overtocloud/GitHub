{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_DC_adminPassword": {
            "defaultValue": null,
            "type": "SecureString"
        },
        "virtualMachines_DC_name": {
            "defaultValue": "DC",
            "type": "String"
        },
        "networkInterfaces_dc209_name": {
            "defaultValue": "dc",
            "type": "String"
        },
        "publicIPAddresses_DC_name": {
            "defaultValue": "DC",
            "type": "String"
        },
        "virtualNetworks_dc_name": {
            "defaultValue": "dc",
            "type": "String"
        },
        "storageAccounts_domainc_name": {
            "defaultValue": "domainc",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "comments": "Generalized from resource: '/subscriptions/b64bfc47-519b-425b-9a47-e7cabc31d840/resourceGroups/ravim-rg/providers/Microsoft.Compute/virtualMachines/DC'.",
            "type": "Microsoft.Compute/virtualMachines",
            "name": "[parameters('virtualMachines_DC_name')]",
            "apiVersion": "2015-06-15",
            "location": "southeastasia",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_D1"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2012-Datacenter",
                        "version": "latest"
                    },
                    "osDisk": {
                        "name": "[parameters('virtualMachines_DC_name')]",
                        "createOption": "FromImage",
                        "vhd": {
                            "uri": "[concat('https', '://', parameters('storageAccounts_domainc_name'), '.blob.core.windows.net', concat('/vhds/', parameters('virtualMachines_DC_name'),'20161016134729.vhd'))]"
                        },
                        "caching": "ReadWrite"
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_DC_name')]",
                    "adminUsername": "testuser",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true
                    },
                    "secrets": [],
                    "adminPassword": "[parameters('virtualMachines_DC_adminPassword')]"
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_dc209_name'))]"
                        }
                    ]
                }
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccounts_domainc_name'))]",
                "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_dc209_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/b64bfc47-519b-425b-9a47-e7cabc31d840/resourceGroups/ravim-rg/providers/Microsoft.Network/networkInterfaces/dc209'.",
            "type": "Microsoft.Network/networkInterfaces",
            "name": "[parameters('networkInterfaces_dc209_name')]",
            "apiVersion": "2016-03-30",
            "location": "southeastasia",
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "properties": {
                            "privateIPAddress": "10.0.6.4",
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_DC_name'))]"
                            },
                            "subnet": {
                                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_dc_name')), '/subnets/dc')]"
                            }
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": []
                },
                "enableIPForwarding": false
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_DC_name'))]",
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_dc_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/b64bfc47-519b-425b-9a47-e7cabc31d840/resourceGroups/ravim-rg/providers/Microsoft.Network/publicIPAddresses/DC'.",
            "type": "Microsoft.Network/publicIPAddresses",
            "name": "[parameters('publicIPAddresses_DC_name')]",
            "apiVersion": "2016-03-30",
            "location": "southeastasia",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "idleTimeoutInMinutes": 4
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/b64bfc47-519b-425b-9a47-e7cabc31d840/resourceGroups/ravim-rg/providers/Microsoft.Network/virtualNetworks/dc'.",
            "type": "Microsoft.Network/virtualNetworks",
            "name": "[parameters('virtualNetworks_dc_name')]",
            "apiVersion": "2016-03-30",
            "location": "southeastasia",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.0.6.0/28"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('virtualNetworks_dc_name')]",
                        "properties": {
                            "addressPrefix": "10.0.6.0/28"
                        }
                    }
                ]
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/b64bfc47-519b-425b-9a47-e7cabc31d840/resourceGroups/ravim-rg/providers/Microsoft.Storage/storageAccounts/domainc'.",
            "type": "Microsoft.Storage/storageAccounts",
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "kind": "Storage",
            "name": "[parameters('storageAccounts_domainc_name')]",
            "apiVersion": "2016-01-01",
            "location": "southeastasia",
            "tags": {},
            "properties": {},
            "resources": [],
            "dependsOn": []
        }
    ]
}
